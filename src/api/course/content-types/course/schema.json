{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "courseCode": {
      "type": "string"
    },
    "courseTitle": {
      "type": "string"
    },
    "classDetails": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "ashoka_departments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ashoka-department.ashoka-department",
      "mappedBy": "courses"
    },
    "semester": {
      "type": "enumeration",
      "enum": [
        "monsoon",
        "summer",
        "spring"
      ]
    },
    "year": {
      "type": "integer"
    },
    "prerequisites": {
      "type": "json"
    },
    "faculties": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::faculty.faculty",
      "inversedBy": "courses"
    },
    "uid": {
      "type": "string",
      "unique": true
    }
  }
}
